<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="invoice.sleepyguyz.com.invoice.MainActivity"
    tools:showIn="@layout/activity_main">

    <Button
        android:id="@+id/invoice"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:background="@color/colorAccent"
        android:text="Invoices"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="29dp" />

    <Button
        android:id="@+id/notes"
        android:layout_width="300dp"
        android:layout_height="44dp"
        android:background="@color/colorAccent"
        android:text="Notes"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="544dp" />

    <Button
        android:id="@+id/support_us"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:background="@color/colorAccent"
        android:text="Support us"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="602dp" />

    <Button
        android:id="@+id/account"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:background="@color/colorAccent"
        android:text="Account"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="663dp" />

    <Button
        android:id="@+id/estimates"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:background="@color/colorAccent"
        android:text="Estimates"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="136dp" />

</android.support.constraint.ConstraintLayout>
